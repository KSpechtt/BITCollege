@model IEnumerable<BITCollege_KS.Models.HonoursState>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr id="multiworld_headings">
        <th>
            @Html.DisplayNameFor(model => model.LowerLimit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpperLimit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TuitionRateFactor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LowerLimit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpperLimit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TuitionRateFactor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.GPAStateId }) |
            @Html.ActionLink("Details", "Details", new { id=item.GPAStateId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.GPAStateId })
        </td>
    </tr>
}

</table>
